<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="Parametro">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_parametro</param>
   </generator>
  </id>
  <property generated="never" lazy="false" name="descricao"/>
  <property generated="never" lazy="false" name="tipo"/>
  <property generated="never" lazy="false" name="requerido"/>
  <property generated="never" lazy="false" name="componente"/>
  <property generated="never" lazy="false" name="dados"/>
  <property generated="never" lazy="false" name="mascara"/>
  <property generated="never" lazy="false" name="rotulo"/>
  <property generated="never" lazy="false" name="px"/>
  <property generated="never" lazy="false" name="py"/>
  <property generated="never" lazy="false" name="operador"/>
  <property name="sequencial"/>
  <property column="referencia_unidade" generated="never" lazy="false" name="refUnidade"/>
  <property generated="never" lazy="false" name="campo"/>
  <many-to-one class="Relatorio" column="cod_relatorio" name="relatorio"/>
 </class>
</hibernate-mapping>
