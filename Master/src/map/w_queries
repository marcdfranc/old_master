Cc
	contaDebito
		retorno: valor da soma dos lançamentos de débito referentyes a uma cc
		parametros:
			unidade: long;
			dataInicio: Date
			dataFim: date;
	end;
	
	contaDebitoHold
		retorno: valor da soma dos lançamentos de débito de todas as unidades
		parametros:
			dataInicio: Date
			dataFim: date;
	end;	
			
	contaCredito
		retorno: valor da soma dos lançamentos de crédito referents a uma cc
		parametros
			unidade: long;
			dataInicio: Date;
			dataFim: Date;
	end;
	
	contaCreditoHold
		retorno: valor da soma dos lançamentos de crédito de todas as unidades
		parametros
			dataInicio: Date;
			dataFim: Date;
	end;
			
	ccLancamento
		retorno: lançamentos referentes a uma unidade no periodo da cc
		prametros
			unidade: long;
			dataInicio: timestamp;
			dataFim: timestamp;
	end;

	ccLancamentoHold
		retorno: lançamentos referentes a todas as unidades no periodo da cc
		prametros
			dataInicio: timestamp;
			dataFim: timestamp;
	end;
end map;

caixa
	caixaFechamento
		retorno: soma dos valores de lançamentos em determinado periodo para determinado usuario 
			que não são fecamento de caixa;
		prametros
			usuario: String;
			inicio: timestamp
			fim: timestamp
		Observação: utilizado para fechamento de caixa;
	end;		
end map;

