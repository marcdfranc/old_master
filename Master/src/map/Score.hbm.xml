<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="Score">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_score</param>
   </generator>
  </id>
  <property name="validade"/>
  <property name="qtde"/>
  <many-to-one class="com.marcsoftware.database.Servico"
   column="cod_servico" name="servico"/>
  <many-to-one class="com.marcsoftware.database.Usuario"
   column="cod_usuario" name="usuario"/>
  <many-to-one class="com.marcsoftware.database.Dependente"
   column="cod_dependente" name="dependente"/>
 </class>
</hibernate-mapping>
