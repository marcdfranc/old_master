<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.marcsoftware.database.LoginUnidade" table="login_unidade">
		<composite-id class="com.marcsoftware.database.LoginUnidadeId" mapped="false" name="id" unsaved-value="undefined">
			<key-many-to-one class="com.marcsoftware.database.Login" column="username" name="login"/>
			<key-many-to-one class="com.marcsoftware.database.Unidade" column="cod_unidade" name="unidade"/>
		</composite-id>
	</class>
</hibernate-mapping>
