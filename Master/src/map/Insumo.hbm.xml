<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="Insumo">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_insumo</param>
   </generator>
  </id>
  <property generated="never" lazy="false" name="ativo"/>
  <property generated="never" lazy="false" name="descricao"/>
  <property generated="never" lazy="false" name="tipo"/>
  <property name="qtde" not-null="true"/>
  <property name="valor" not-null="true"/>
  <many-to-one class="com.marcsoftware.database.Ramo" column="cod_ramo" name="ramo"/>
  <many-to-one class="com.marcsoftware.database.Unidade"
   column="cod_unidade" name="unidade"/>
 </class>
</hibernate-mapping>
