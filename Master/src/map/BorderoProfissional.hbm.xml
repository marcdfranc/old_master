<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="BorderoProfissional" table="bordero">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_bordero_profissional</param>
   </generator>
  </id>
  <property generated="never" lazy="false" name="inicio"/>
  <property generated="never" lazy="false" name="fim"/>
  <property generated="never" lazy="false" name="cadastro"/>
  <property column="doc_digital" name="docDigital"/>
  <many-to-one class="com.marcsoftware.database.Pessoa"
   column="cod_pessoa" name="pessoa"/>
  <many-to-one class="com.marcsoftware.database.FormaPagamento"
   column="cod_forma_pagamento" name="formaPagamento"/>
 </class>
</hibernate-mapping>
