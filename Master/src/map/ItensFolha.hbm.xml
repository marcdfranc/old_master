<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="ItensFolha" table="itens_folha">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_encargo</param>
   </generator>
  </id>
  <many-to-one class="com.marcsoftware.database.Folha"
   column="cod_folha" name="folha"/>
  <many-to-one class="com.marcsoftware.database.Encargo"
   column="cod_encargo" name="encargo"/>
 </class>
</hibernate-mapping>
