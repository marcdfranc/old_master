<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="CartaoAcesso" table="cartao_acesso">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_tabela_acesso</param>
   </generator>
  </id>
  <property generated="never" lazy="false" name="numero"/>
  <property generated="never" lazy="false" name="status"/>
  <property column="indice" name="index"/>
  <many-to-one class="com.marcsoftware.database.Login" column="username" name="login"/>
 </class>
</hibernate-mapping>
