<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="Concessionaria">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_acesso</param>
   </generator>
  </id>
  <property column="numero" generated="never" lazy="false" name="numero"/>
  <property column="descricao" generated="never" lazy="false" name="descricao"/>
 </class>
</hibernate-mapping>
