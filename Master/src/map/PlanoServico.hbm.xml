<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="PlanoServico" table="plano_servico">
  <composite-id class="PlanoServicoId" mapped="false" name="id" unsaved-value="undefined">
   <key-many-to-one class="Servico" column="cod_servico" name="servico"/>
   <key-many-to-one class="Plano" column="cod_plano" name="plano"/>
  </composite-id>
  <property name="qtde"/>
 </class>
</hibernate-mapping>
