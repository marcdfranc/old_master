<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="Ponto">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_encargo</param>
   </generator>
  </id>
  <property name="data"/>
  <property name="entrada"/>
  <property name="saida"/>
  <property name="horas"/>
  <many-to-one class="com.marcsoftware.database.Folha" name="folha"/>
 </class>
</hibernate-mapping>
