<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="DebitoAnterior">
  <composite-id class="FaturaEmpresaId" mapped="false" name="id" unsaved-value="undefined">
   <key-many-to-one class="Pessoa" column="cod_pessoa" name="pessoa"/>
   <key-many-to-one class="com.marcsoftware.database.Lancamento"
    column="cod_lancamento" name="lancamento"/>
  </composite-id>
 </class>
</hibernate-mapping>
