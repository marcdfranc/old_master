<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">

	<joined-subclass name="Juridica" extends="Pessoa">
		<key column="codigo"/>
		
		<property name="razaoSocial" not-null="true">
			<column name="razao_social"/>
		</property>
		
		<property name="fantasia"/>
		<property name="cnpj" not-null="true"/>
		<property name="ie"/>						
	</joined-subclass>
</hibernate-mapping>