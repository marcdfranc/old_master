<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="Folha">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_encargo</param>
   </generator>
  </id>
  <property name="inicio"/>
  <property name="fim"/>
  <property name="status"/>
  <many-to-one class="com.marcsoftware.database.Funcionario"
   column="cod_funcionario" name="funcionario"/>
 </class>
</hibernate-mapping>
