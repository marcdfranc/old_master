<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.marcsoftware.database">
 <class name="Boleto">
  <id name="codigo">
   <generator class="native">
    <param name="sequence">sq_boleto</param>
   </generator>
  </id>
  <property generated="never" lazy="false" name="vencimento"/>
  <property generated="never" lazy="false" name="emissao"/>
  <property generated="never" lazy="false" name="valor"/>
  <property generated="never" lazy="false" name="status"/>
  <many-to-one class="com.marcsoftware.database.Pessoa"
   column="cod_pessoa" name="pessoa"/>
 </class>
</hibernate-mapping>
