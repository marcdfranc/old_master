	<queryString>
		<![CDATA[SELECT SUM(l.valor_pago) AS total, SUM(c.valor_pago) AS valor_cliente, COUNT(i.*) AS guias, l.cod_unidade
FROM itens_bordero i
INNER JOIN lancamento l ON(i.cod_lancamento_operacional = l.codigo)
INNER JOIN lancamento c ON(i.cod_lancamento_cliente = c.codigo)
WHERE l.cod_unidade = $P{COD_UNIDADE}
AND (l.data_quitacao BETWEEN $P{DATA_INICIAL} and $P{DATA_FINAL})
GROUP BY l.cod_unidade]]>
	</queryString>
