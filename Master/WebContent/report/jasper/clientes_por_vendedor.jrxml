	<queryString>
		<![CDATA[SELECT u.codigo, f.nome, f.cpf, f.nascimento, c.ctr, i.descricao AS fone, p.cadastro,
	u.renovacao, e.referencia AS ref_unidade, p.cod_unidade,
	s.codigo as cod_funcionario, s.nome as funcionario,
	(select count(l.*) from parcela_orcamento a
	inner join orcamento o ON(a.cod_orcamento = o.codigo)
	INNER JOIN lancamento l ON(a.cod_lancamento = l.codigo)
	where l.status = 'a'
	AND l.vencimento < CURRENT_DATE
	AND o.cod_usuario = u.codigo) AS parcela_atraso,
	(Select count(n.*) from mensalidade m 
	inner join lancamento n ON(m.cod_lancamento = n.codigo)
	where n.vencimento < CURRENT_DATE
	AND n.status = 'a'
	AND m.cod_usuario = u.codigo) AS mensalidade_atraso	
FROM usuario AS u
INNER JOIN fisica AS f ON(u.codigo = f.codigo)
INNER JOIN contrato AS c ON(u.cod_contrato = c.codigo)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
INNER JOIN pessoa AS e ON(p.cod_unidade = e.codigo)
INNER JOIN fisica AS s ON(c.cod_funcionario = s.codigo)
WHERE i.principal = 's'
AND p.cod_unidade = 78
ORDER BY s.codigo, p.cod_unidade, p.cadastro DESC, f.nome]]>
	</queryString>
