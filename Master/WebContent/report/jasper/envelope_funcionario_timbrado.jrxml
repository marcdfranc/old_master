	<queryString>
		<![CDATA[SELECT f.nome, e.rua_av,
CASE
 WHEN e.numero ISNULL THEN 'Sem Numero'
 ELSE e.numero
 END AS numero, e.bairro, e.cidade, UPPER(e.uf) AS uf, e.complemento, e.cep, n.report_logo
FROM funcionario AS u
	INNER JOIN endereco AS e ON(e.cod_pessoa = u.codigo)
	INNER JOIN fisica AS f ON(u.codigo = f.codigo)
	INNER JOIN pessoa s ON (s.codigo = e.cod_pessoa)
	INNER JOIN unidade n ON (n.codigo = s.cod_unidade)
WHERE n.codigo = 1293]]>
	</queryString>
