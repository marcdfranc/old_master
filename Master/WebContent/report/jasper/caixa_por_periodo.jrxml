	<queryString>
		<![CDATA[SELECT c.codigo, f.nome, c.inicio, c.fim, c.valor_inicial, c.valor_final, c.usuario, c.status, c.cod_unidade,
(SELECT SUM(l.valor_pago) FROM lancamento l WHERE l.tipo = 'c' 
   AND to_char(l.data_quitacao, 'YYYY-MM-DD') = to_char(c.inicio, 'YYYY-MM-DD')
   AND l.recebimento = c.usuario) AS entrada,
(SELECT SUM(l.valor_pago) FROM lancamento l WHERE l.tipo = 'd' 
   AND to_char(l.data_quitacao, 'YYYY-MM-DD') = to_char(c.inicio, 'YYYY-MM-DD')
   AND l.recebimento = c.usuario) AS saida
FROM caixa c
INNER JOIN pessoa p ON(p.username = c.usuario)
INNER JOIN fisica f ON(f.codigo = p.codigo)
where c.codigo IN(26, 1694, 1693, 1689, 1685)
ORDER BY c.usuario]]>
