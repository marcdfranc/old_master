	<queryString>
		<![CDATA[SELECT t.cod_unidade, t.valor_operacional, t.valor_cliente, s.referencia AS cod_servico,
	s.descricao AS servico, v.descricao AS vigencia,
	CASE
		WHEN e.setor = 'o' THEN 'Odontológico'
		WHEN e.setor = 'l' THEN 'Laboratorial'
		WHEN e.setor = 'm' THEN 'Médico'
		WHEN e.setor = 'h' THEN 'Hospitalar'
	END AS setor, e.codigo AS cod_especialidade, e.descricao AS especialidade
FROM tabela AS t
INNER JOIN vigencia AS v ON(t.cod_vigencia = v.codigo)
INNER JOIN servico AS s ON(t.cod_servico = s.codigo)
INNER JOIN especialidade AS e ON(s.cod_especialidade  = e.codigo)
WHERE t.cod_unidade = 78
ORDER BY e.descricao, s.ref]]>
	</queryString>
