	<parameter name="CONTEXT_PATH" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["C:/Users/marcelo/Documents/Sistemas/master/master.com/WebContent/report/"]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT b.codigo, b.vencimento, b.emissao, b.valor AS valor_total,
	CASE
		WHEN s.nome ISNULL THEN r.razao_social
		ELSE s.nome
	END AS sacado,
	CASE
		WHEN s.cpf ISNULL THEN r.cnpj
		ELSE s.cpf
	END AS cpf_cnpj,
	CASE
		WHEN a.cod_contrato ISNULL THEN r.codigo
		ELSE t.ctr
	END AS ctr, e.rua_av, e.numero, e.bairro, e.cep, e.complemento,
	e.cidade, UPPER(e.uf) AS uf, j.razao_social, j.cnpj, u.report_logo,
	u.descricao AS unidade, o.referencia AS ref_unidade,
	u.codigo AS cod_unidade, f.descricao AS fone_unidade, c.cod_banco,
	c.agencia, c.numero AS cc, c.carteira, c.vlr_boleto, n.descricao AS banco,
	CASE
		WHEN a.cod_contrato ISNULL THEN 'j'
		ELSE 'f'
	END AS tipo_pessoa
FROM boleto AS b
INNER JOIN pessoa AS p ON(b.cod_pessoa = p.codigo)
INNER JOIN endereco AS e ON(e.cod_pessoa = b.cod_pessoa)
INNER JOIN unidade AS u ON(p.cod_unidade = u.codigo)
INNER JOIN juridica AS j ON(j.codigo = u.codigo)
INNER JOIN pessoa AS o ON(o.codigo = u.codigo)
INNER JOIN informacao AS f ON(f.cod_pessoa = u.codigo)
INNER JOIN conta AS c ON(c.cod_pessoa = u.codigo)
INNER JOIN banco AS n ON(c.cod_banco = n.codigo)
LEFT JOIN fisica AS s ON(b.cod_pessoa = s.codigo)
LEFT JOIN juridica AS r ON(b.cod_pessoa = r.codigo)
LEFT JOIN usuario AS a ON(b.cod_pessoa = a.codigo)
LEFT JOIN contrato AS t ON(a.cod_contrato = t.codigo)
LEFT JOIN empresa AS m ON(b.cod_pessoa = m.codigo)
WHERE f.principal = 's'
and b.codigo in(204575,204576,204577,204578,204579,204580,204581,204582,204583,204584,204585,204586)
and c.codigo = 104]]>
