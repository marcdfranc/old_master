	<queryString>
		<![CDATA[SELECT f.codigo AS cod_usuario, f.nome, f.cpf, i.ctr, i.cod_ocorrencia, i.ocorrencia,
  c.cod_unidade, c.descricao AS arquivo, c.cadastro
FROM ccob_itens i
INNER JOIN cobranca_ccob c ON(i.cod_cobranca = c.codigo)
INNER JOIN contrato o ON(c.cod_unidade = o.cod_unidade AND i.ctr = o.ctr)
INNER JOIN usuario u ON(u.cod_contrato = o.codigo)
INNER JOIN fisica f ON(u.codigo = f.codigo)
WHERE i.cod_cobranca = $P{COD_COBRANCA}
ORDER BY i.cod_ocorrencia]]>
	</queryString>
