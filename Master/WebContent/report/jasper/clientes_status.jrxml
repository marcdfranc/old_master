	<queryString>
		<![CDATA[SELECT u.codigo, c.ctr, f.nome, d.atraso, f.cpf,
  p.cadastro, p.cod_unidade
FROM usuario u
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN fisica AS f ON(u.codigo = f.codigo)
LEFT JOIN vw_debito_mensalidade AS d ON (d.usuario = u.codigo)
LEFT JOIN contrato c ON(u.cod_contrato = c.codigo)
ORDER BY p.cod_unidade, p.cadastro DESC, f.nome]]>
	</queryString>
