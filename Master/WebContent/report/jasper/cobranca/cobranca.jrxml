	<queryString>
		<![CDATA[SELECT v.ctr, v.nome, v.cadastro, v.rua, v.numero, v.cidade , v.cep, v.complemento,
	v.bairro, v.parcelas, v.informacao, v.usuario, v.plano, v.empresa,
	v.vencimento, v.atraso, e.cidade AS cidade_unidade, UPPER(e.uf) AS uf,
	i.descricao AS fone_unidade
FROM vw_debito_mensalidade AS v
	INNER JOIN pessoa AS p ON(v.usuario = p.codigo)
	INNER JOIN unidade AS u ON(p.cod_unidade = u.codigo)
	INNER JOIN endereco AS e ON(e.cod_pessoa = u.codigo)
	INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
	WHERE i.principal = 's']]>
	</queryString>
