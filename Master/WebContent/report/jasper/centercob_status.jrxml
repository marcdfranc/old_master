	<queryString>
		<![CDATA[SELECT c.ctr, f.nome, f.cpf, f.nascimento, u.ccob_status, p.cod_unidade, p.cadastro,
  CASE 
    WHEN u.ccob_status='N' THEN 'Não Enviar - Pag. Esc.'
    WHEN u.ccob_status='I' THEN 'Para Enviar'
    WHEN u.ccob_status='E' THEN 'Enviado'
    WHEN u.ccob_status='A' THEN 'Para Alterar - UC'
    WHEN u.ccob_status='C' THEN 'Para Cancelar'
    WHEN u.ccob_status='L' THEN 'Cancelado'
    WHEN u.ccob_status='R' THEN 'Com erro de Dados'
    WHEN u.ccob_status='D' THEN 'Com Contas em Atraso'
    ELSE 'Nenhum'
  END AS descricao_status,
(
  SELECT descricao FROM informacao AS i 
  WHERE i.principal = 's' 
    AND i.cod_pessoa = u.codigo LIMIT 1) AS informacao 
FROM usuario AS u
INNER JOIN contrato AS c ON(u.cod_contrato = c.codigo)
INNER JOIN fisica AS f ON(u.codigo = f.codigo)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
ORDER BY ccob_status]]>
	</queryString>
