	<queryString>
		<![CDATA[SELECT v.ano_quitacao, mes_quitacao, l.cod_unidade, SUM(l.valor_pago) AS valor_cliente,

	(SELECT SUM(a.valor_pago) FROM itens_bordero AS t
	INNER JOIN lancamento AS a ON(t.cod_lancamento_operacional = a.codigo)
	INNER JOIN lancamento AS n ON(t.cod_lancamento_cliente = n.codigo)
	INNER JOIN vw_lancamento_datas AS w ON(w.cod_lancamento = n.codigo)
	WHERE w.ano_quitacao = v.ano_quitacao
	AND w.mes_quitacao = v.mes_quitacao
	AND a.cod_unidade = l.cod_unidade
	AND a.status IN('q', 'f')) AS valor_operacional


FROM parcela_orcamento AS p
INNER JOIN lancamento AS l ON(l.codigo = p.cod_lancamento)
INNER JOIN vw_lancamento_datas AS v ON(v.cod_lancamento = p.cod_lancamento)
WHERE l.status IN('f', 'q')
AND l.cod_unidade = $P{COD_UNIDADE}
AND l.tipo = 'c'
AND l.data_quitacao BETWEEN $P{INICIO} AND $P{FIM}
GROUP BY v.ano_quitacao, v.mes_quitacao, l.cod_unidade
ORDER BY v.ano_quitacao, v.mes_quitacao]]>
	</queryString>
