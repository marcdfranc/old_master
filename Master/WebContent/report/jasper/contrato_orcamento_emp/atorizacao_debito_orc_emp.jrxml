	<queryString>
		<![CDATA[SELECT o.codigo, e.referencia AS ref_unidade, j.fantasia, j.cnpj, p.cod_unidade,
	SUM(l.valor) AS valor, COUNT(l.*) AS parcelas, c.ctr, n.cidade, n.uf,
	CASE
		WHEN d.codigo ISNULL THEN f.nome
		ELSE d.nome
	END AS nome,
	CASE
		WHEN d.codigo ISNULL THEN f.cpf
		ELSE d.cpf
	END AS cpf
FROM orcamento AS o
INNER JOIN pessoa AS p ON(o.cod_pessoa = p.codigo)
INNER JOIN pessoa AS e ON(p.cod_unidade = e.codigo)
INNER JOIN juridica AS j ON(p.cod_unidade = j.codigo)
INNER JOIN endereco AS n ON(p.cod_unidade = n.cod_pessoa)
INNER JOIN parcela_orcamento AS a ON(o.codigo = a.cod_orcamento)
INNER JOIN lancamento AS l ON(a.cod_lancamento = l.codigo)
INNER JOIN fisica AS f ON(o.cod_usuario = f.codigo)
LEFT JOIN dependente AS d ON(o.cod_dependente = d.codigo)
INNER JOIN usuario AS u ON(o.cod_usuario = u.codigo)
INNER JOIN contrato AS c ON(u.cod_contrato = c.codigo)
WHERE o.codigo = $P{COD_ORCAMENTO}
GROUP BY o.codigo, o.parcelas, e.referencia, j.fantasia, j.cnpj, c.ctr, p.cod_unidade,
	d.codigo, f.nome, d.nome, f.cpf, d.cpf, n.cidade, n.uf]]>
