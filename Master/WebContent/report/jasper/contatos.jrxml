	<queryString>
		<![CDATA[SELECT i.cod_grupo, g.descricao AS grupo, g.username, c.nome, c.aniverssario,
p.cod_unidade, n.tipo, n.descricao, n.principal
FROM itens_grupo as i
INNER JOIN catalogo AS c ON(i.cod_catalogo = c.codigo)
INNER JOIN grupo AS g ON(i.cod_grupo = g.codigo)
INNER JOIN informacao_catalogo as n ON(n.cod_catalogo = c.codigo)
INNER JOIN pessoa AS p ON(p.username = g.username)
ORDER BY g.descricao, c.nome]]>
