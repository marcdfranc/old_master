	<queryString>
		<![CDATA[SELECT INITCAP(f.nome) AS paciente, u.ctr, INITCAP(n.descricao) AS unidade, o.descricao AS atendimento, 
	e.uf, s.referencia AS cod_unidade, 
	(SELECT INITCAP(d.nome) FROM dependente AS d
		WHERE d.cod_usuario = u.codigo
			AND d.referencia = $P{COD_DEPENDENTE}) AS dependente
FROM usuario AS u
	INNER JOIN fisica AS f ON(f.codigo = u.codigo)
	INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
	INNER JOIN unidade AS n ON(p.cod_unidade = n.codigo)
	INNER JOIN informacao AS o ON(o.cod_pessoa = n.codigo)
	INNER JOIN endereco AS e ON(e.cod_pessoa = n.codigo)
	INNER JOIN pessoa AS s ON(n.codigo = s.codigo)	
WHERE o.principal = 's'	
	AND u.ctr = $P{CTR_CLIENTE}]]>
	</queryString>
