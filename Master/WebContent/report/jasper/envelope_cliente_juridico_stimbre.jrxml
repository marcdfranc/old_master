				<textElement>
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[com.marcsoftware.Util.initCap($F{fantasia})]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="42" y="223" width="288" height="16"/>
				<textElement>
					<font fontName="Arial" size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[com.marcsoftware.Util.initCap($F{bairro}) + " - " +
com.marcsoftware.Util.initCap($F{cidade}) +  "/" + $F{uf}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="42" y="207" width="288" height="16"/>
				<textElement>
					<font fontName="Arial" size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[($F{complemento}.equals( null ) || $F{complemento}.equals( "" ))?
    com.marcsoftware.Util.initCap($F{rua_av}) + ", " +
    $F{numero} : com.marcsoftware.Util.initCap($F{rua_av}) +
    ", " + $F{numero} + " - " + $F{complemento}]]></textFieldExpression>
			</textField>
			<textField pattern="">
				<reportElement x="42" y="239" width="189" height="16"/>
				<textElement>
					<font fontName="Arial" size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{cep}.substring( 0, 5 ) + "-" + $F{cep}.substring( 5, $F{cep}.length() )]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
