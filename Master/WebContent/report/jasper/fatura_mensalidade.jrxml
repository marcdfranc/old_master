	<queryString>
		<![CDATA[SELECT c.cod_banco AS banco, c.agencia, c.numero AS cc, l.vencimento, l.valor, l.taxa,
	l.codigo, f.nome, f.cpf, e.rua_av, e.numero, e.bairro, e.cep, e.complemento,
	e.cidade, UPPER(e.uf) AS uf, j.cnpj, j.razao_social, n.descricao,
	s.referencia AS ref_unidade, i.descricao AS fone_unidade, u.ctr,
	m.codigo AS mensalidade
FROM usuario AS u
INNER JOIN fisica AS f ON(f.codigo = u.codigo)
INNER JOIN pessoa AS p ON(p.codigo = u.codigo)
INNER JOIN endereco AS e ON(e.cod_pessoa = u.codigo)
INNER JOIN unidade AS n ON(p.cod_unidade = n.codigo)
INNER JOIN juridica AS j ON(j.codigo = n.codigo)
INNER JOIN pessoa AS s ON(n.codigo = s.codigo)
INNER JOIN conta AS c ON(c.cod_pessoa = n.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = n.codigo)
INNER JOIN mensalidade AS m ON(m.cod_usuario = u.codigo)
INNER JOIN Lancamento AS l ON(m.cod_lancamento = l.codigo)
WHERE u.codigo = 545
AND i.principal = 's'
AND l.status in('a', 'e', 'n')
ORDER BY u.codigo, u.ctr, l.vencimento]]>
	</queryString>
