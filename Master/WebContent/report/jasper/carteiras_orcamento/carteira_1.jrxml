	<queryString>
		<![CDATA[SELECT u.codigo AS usuario, v.parcelas, e.cep, e.rua_av, e.numero,
	e.bairro, e.complemento, e.cidade, e.uf, p.cod_unidade,
	p.cadastro, i.descricao AS fone_titular,
	CASE
		WHEN o.cod_dependente ISNULL THEN 'titular'
		ELSE d.parentesco
	END AS parentesco,
	CASE
		WHEN o.cod_dependente ISNULL THEN i.descricao
		ELSE d.fone
	END AS fone,
	CASE
		WHEN o.cod_dependente ISNULL THEN c.ctr || '-0'
		ELSE c.ctr || '-' || d.referencia
	END AS ctr,
	CASE
		WHEN o.cod_dependente ISNULL THEN f.nome
		ELSE d.nome
	END AS nome,
	CASE
		WHEN o.cod_dependente ISNULL THEN f.cpf
		ELSE d.cpf
	END AS cpf,
	CASE
		WHEN o.cod_dependente ISNULL THEN f.nascimento
		ELSE d.nascimento
	END AS nascimento
FROM usuario AS u
INNER JOIN fisica AS f ON(f.codigo = u.codigo)
INNER JOIN pessoa AS p ON(p.codigo = u.codigo)
INNER JOIN endereco AS e ON(e.cod_pessoa = u.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
INNER JOIN contrato AS c ON(c.codigo = u.cod_contrato)
INNER JOIN orcamento AS o ON(o.cod_usuario = u.codigo)
LEFT JOIN dependente AS d ON(d.codigo = o.cod_dependente)
INNER JOIN vw_debito_orcamento AS v ON(v.cod_orcamento = o.codigo)
WHERE i.principal = 's'
ORDER BY e.cidade, e.bairro, c.ctr, d.referencia]]>
	</queryString>
