	<queryString language="SQL">
		<![CDATA[SELECT INITCAP(f.nome) AS nome, f.cpf, r.conselho, 
	CASE 
		WHEN e.setor = 'o' THEN 'Odontológica'
		WHEN e.setor = 'l' THEN 'Laboratorial'
		WHEN e.setor = 'm' THEN 'Médica'
		WHEN e.setor = 'h' THEN 'Hospitalar'
	END AS setor, 
	INITCAP(e.descricao) AS especialidade, b.codigo AS fatura,
	CASE
		WHEN TO_CHAR(b.fim, 'mm') = '01' THEN 'Janeiro'
		WHEN TO_CHAR(b.fim, 'mm') = '02' THEN 'Fevereiro'
		WHEN TO_CHAR(b.fim, 'mm') = '03' THEN 'Março'
		WHEN TO_CHAR(b.fim, 'mm') = '04' THEN 'Abril'
		WHEN TO_CHAR(b.fim, 'mm') = '05' THEN 'Maio'
		WHEN TO_CHAR(b.fim, 'mm') = '06' THEN 'Junho'
		WHEN TO_CHAR(b.fim, 'mm') = '07' THEN 'Julho'
		WHEN TO_CHAR(b.fim, 'mm') = '08' THEN 'Agosto'
		WHEN TO_CHAR(b.fim, 'mm') = '09' THEN 'Setembro'
		WHEN TO_CHAR(b.fim, 'mm') = '10' THEN 'Outubro'
		WHEN TO_CHAR(b.fim, 'mm') = '11' THEN 'Novembro'
		WHEN TO_CHAR(b.fim, 'mm') = '12' THEN 'Dezembro'
	END AS mes_base, TO_CHAR(b.fim, 'yyyy') AS ano_base, i.cod_lancamento_operacional AS lc_operacional, 
	i.cod_lancamento_cliente AS lc_cliente, 
	CASE
		WHEN o.cod_dependente ISNULL THEN s.referencia || '-0'
		ELSE
		s.referencia || '-' || d.referencia
	END AS ctr, o.codigo AS orcamento, o.parcelas, 
	(SELECT COUNT(*) FROM parcela_orcamento AS m
		INNER JOIN lancamento AS t ON(m.cod_lancamento = t.codigo)
	WHERE m.cod_orcamento = o.codigo
		AND t.status IN ('q', 'f')) AS pagas,
	CASE
		WHEN o.cod_dependente ISNULL THEN INITCAP(c.nome)
		ELSE
		INITCAP(d.nome)
	END AS cliente, l.valor_pago AS operacional, n.emissao, INITCAP(j.fantasia) AS fantasia, a.referencia AS unidade
FROM bordero AS b
	INNER JOIN itens_bordero AS i ON(i.cod_bordero = b.codigo)
	INNER JOIN lancamento AS l ON(i.cod_lancamento_operacional = l.codigo)
	INNER JOIN parcela_orcamento AS p ON(i.cod_lancamento_cliente = p.cod_lancamento)
	INNER JOIN orcamento AS o ON(p.cod_orcamento = o.codigo)
	INNER JOIN profissional AS r ON(o.cod_profissional = r.codigo)
	INNER JOIN fisica AS f ON(r.codigo = f.codigo)
	INNER JOIN especialidade AS e ON(r.cod_especialidade = e.codigo)
	INNER JOIN pessoa AS s ON(o.cod_usuario = s.codigo)
	INNER JOIN fisica AS c ON(o.cod_usuario = c.codigo)
	LEFT JOIN dependente AS d ON(o.cod_dependente = d.codigo)
	INNER JOIN lancamento AS n ON(i.cod_lancamento_cliente = n.codigo)
	INNER JOIN juridica AS j ON(s.cod_unidade = j.codigo)
	INNER JOIN pessoa AS a ON(j.codigo = a.codigo)
WHERE (1 = 1) 
	AND b.codigo = $P{FATURA}
ORDER BY b.codigo, o.codigo, f.nome, l.data_quitacao]]>
	</queryString>
