	<queryString>
		<![CDATA[SELECT i.tipo, i.descricao AS info, e.rua_av AS endereco_unidade,
	e.numero AS numero_unidade, e.bairro AS bairro_unidade,
	e.complemento AS complemento_unidade, e.cep AS cep_unidade,
	e.cidade AS cidade_unidade, UPPER(e.uf) AS uf_unidade, report_logo,
	(SELECT descricao FROM informacao
		WHERE tipo = 'email'
		AND cod_pessoa = i.cod_pessoa) AS email_unidade,
	(SELECT descricao FROM informacao
		WHERE principal = 's'
		AND cod_pessoa = i.cod_pessoa) AS fone_unidade,
	u.site AS site_unidade, u.codigo AS cod_unidade
FROM informacao AS i
INNER JOIN unidade AS u ON(i.cod_pessoa = u.codigo)
INNER JOIN endereco AS e ON(e.cod_pessoa = i.cod_pessoa)
WHERE i.cod_pessoa = 1294]]>
	</queryString>
