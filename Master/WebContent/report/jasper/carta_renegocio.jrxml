	<queryString>
		<![CDATA[SELECT c.nome, c.rua_av, c.numero, c.bairro, c.cidade, UPPER(c.uf) AS uf, c.complemento,
	c.cep, i.descricao AS fone_unidade, e.rua_av AS endereco_unidade, c.cod_unidade,
	e.numero AS numero_unidade, e.bairro AS bairro_unidade,
	e.complemento AS complemento_unidade, e.cep AS cep_unidade,
	e.cidade AS cidade_unidade, UPPER(e.uf) AS uf_unidade, j.cnpj,
	(SELECT o.descricao FROM informacao AS o
		WHERE o.tipo = 'email'
		AND o.cod_pessoa = c.cod_unidade) AS email_unidade,
	(SELECT r.descricao FROM informacao AS r
		WHERE r.tipo = 'site'
		AND r.cod_pessoa = c.cod_unidade) AS site_unidade
FROM vw_cliente_completo AS c
INNER JOIN informacao AS i ON(i.cod_pessoa = c.cod_unidade)
INNER JOIN juridica AS j ON(c.cod_unidade = j.codigo)
INNER JOIN endereco AS e ON(e.cod_pessoa = c.cod_unidade)
WHERE (i.principal = 's')]]>
	</queryString>
