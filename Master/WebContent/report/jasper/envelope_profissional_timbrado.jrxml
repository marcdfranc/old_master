	<queryString>
		<![CDATA[SELECT f.nome, e.rua_av, e.numero, e.bairro, e.cidade, UPPER(e.uf) AS uf, e.complemento, e.cep, u.report_logo
FROM profissional AS p
	INNER JOIN endereco AS e ON(e.cod_pessoa = p.codigo)
	INNER JOIN fisica AS f ON(p.codigo = f.codigo)
	INNER JOIN pessoa s ON (s.codigo = e.cod_pessoa)
	INNER JOIN unidade u ON (u.codigo = s.cod_unidade)
where u.codigo = 1293]]>
	</queryString>
