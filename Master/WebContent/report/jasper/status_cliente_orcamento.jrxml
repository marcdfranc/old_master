	<queryString>
		<![CDATA[SELECT o.codigo AS cod_orcamento, o.data, v.valor_cliente, c.ctr, e.cod_unidade,
	CASE
		WHEN o.cod_dependente ISNULL THEN '0'
		ELSE d.referencia
	END AS ref_ctr,
	CASE
		WHEN o.cod_dependente ISNULL THEN f.nome
		ELSE d.nome
	END AS nome_usuario,
	CASE
		WHEN j.codigo ISNULL THEN i.nome
		ELSE j.fantasia
	END AS prof_fantasia,
	CASE
		WHEN v.status = 'q' THEN 'Concluído'
		WHEN v.status = 'c' THEN 'Cancelado'
		WHEN v.status = 't' THEN 'Em Tratamento'
		ELSE 'Em Aberto'
	END AS status
FROM orcamento AS o
INNER JOIN vw_total_orcamento AS v ON(o.codigo = v.cod_orcamento)
INNER JOIN fisica AS f ON(o.cod_usuario = f.codigo)
INNER JOIN usuario AS u ON(o.cod_usuario = u.codigo)
INNER JOIN pessoa AS e ON(o.cod_usuario = e.codigo)
INNER JOIN contrato AS c ON(u.cod_contrato = c.codigo)
LEFT JOIN fisica AS i ON(o.cod_pessoa = i.codigo)
LEFT JOIN juridica AS j ON(o.cod_pessoa = j.codigo)
LEFT JOIN dependente AS d ON(o.cod_dependente = d.codigo)
WHERE e.cod_unidade = 78
ORDER BY o.data DESC, c.ctr, d.referencia]]>
	</queryString>
