	<queryString>
		<![CDATA[SELECT CAST(v.ano_cadastro AS INTEGER) AS ano_cadastro, v.mes_cadastro, c.cod_unidade,
	COUNT(v.cod_usuario) AS usuarios, CAST(v.ano_cadastro AS INTEGER) - 1 AS ano1,
	CAST(v.ano_cadastro AS INTEGER) - 2 AS ano2,
	CAST(v.ano_cadastro AS INTEGER) - 3 AS ano3,
	(SELECT COUNT(w.cod_usuario)
	FROM vw_cliente_datas AS w
	INNER JOIN pessoa AS p ON(w.cod_usuario = p.codigo)
	WHERE w.mes_cadastro = v.mes_cadastro
	AND p.cod_unidade = c.cod_unidade
	AND w.ano_cadastro = CAST(v.ano_cadastro AS INTEGER) - 1) as usuarios1,

	(SELECT COUNT(l.cod_usuario)
	FROM vw_cliente_datas AS l
	INNER JOIN pessoa AS e ON(l.cod_usuario = e.codigo)
	WHERE l.mes_cadastro = v.mes_cadastro
	AND e.cod_unidade = c.cod_unidade
	AND l.ano_cadastro = CAST(v.ano_cadastro AS INTEGER) - 2) as usuarios2,

	(SELECT COUNT(i.cod_usuario)
	FROM vw_cliente_datas AS i
	INNER JOIN pessoa AS s ON(i.cod_usuario = s.codigo)
	WHERE i.mes_cadastro = v.mes_cadastro
	AND s.cod_unidade = c.cod_unidade
	AND i.ano_cadastro = CAST(v.ano_cadastro AS INTEGER) - 3) as usuarios3

FROM vw_cliente_datas AS v
INNER JOIN contrato AS c ON(v.cod_contrato = c.codigo)
WHERE v.cadastro BETWEEN $P{INICIO} AND $P{FIM}
AND c.cod_unidade = $P{COD_UNIDADE}
GROUP BY v.mes_cadastro, c.cod_unidade, v.ano_cadastro
order by v.mes_cadastro]]>
	</queryString>
