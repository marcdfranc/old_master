<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="envelope_dependentes_timbrado" language="groovy" pageWidth="652" pageHeight="321" columnWidth="612" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<parameter name="CONTEXT_PATH" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["C:/Users/marcelo/Documents/Sistemas/master/master.com/WebContent/report/"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT d.nome, u.rua_av, u.numero, u.bairro, u.cidade, UPPER(u.uf) AS uf, u.complemento, u.cep, n.report_logo
FROM dependente AS d
INNER JOIN vw_cliente_completo AS u ON(d.cod_usuario = u.usuario)
INNER JOIN usuario s ON (s.codigo = d.cod_usuario)
INNER JOIN contrato c ON (c.codigo = s.cod_contrato)
INNER JOIN unidade n ON (n.codigo = c.cod_unidade)
where n.codigo = 1293]]>
	</queryString>
