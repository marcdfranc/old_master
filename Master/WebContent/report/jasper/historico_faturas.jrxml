	<queryString>
		<![CDATA[select b.cod_profissional, f.nome, p.cod_unidade, u.descricao,
  e.referencia, u.descricao, b.valor, b.cadastro, b.codigo AS fatura
from vw_lista_bordero AS b
INNER JOIN fisica f ON(b.cod_profissional = f.codigo)
INNER JOIN pessoa p ON(f.codigo = p.codigo)
INNER JOIN unidade AS u ON(p.cod_unidade = u.codigo)
INNER JOIN pessoa AS e ON(e.codigo = u.codigo)
ORDER BY b.cod_profissional, b.cadastro DESC]]>
	</queryString>
