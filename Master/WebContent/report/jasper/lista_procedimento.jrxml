	<queryString>
		<![CDATA[SELECT s.referencia, s.descricao, e.codigo AS cod_especialidade,
	CASE
		WHEN e.setor = 'o' THEN 'Odontológica'
		WHEN e.setor = 'l' THEN 'Laboratorial'
		WHEN e.setor = 'm' THEN 'Médica'
		WHEN e.setor = 'h' THEN 'Hospitalar'
	END AS setor, e.descricao AS especialidade
FROM servico AS s
INNER JOIN especialidade AS e ON(s.cod_especialidade = e.codigo)
ORDER BY e.descricao, s.descricao]]>
	</queryString>
