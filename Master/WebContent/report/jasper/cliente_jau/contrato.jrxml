	<queryString>
		<![CDATA[SELECT c.codigo AS contrato, c.ctr, c.cod_funcionario, p.referencia AS ref_unidade,
	c.cod_unidade, e.cidade, UPPER(e.uf) AS uf
FROM contrato AS c
INNER JOIN unidade AS u ON(c.cod_unidade = u.codigo)
INNER JOIN pessoa AS p ON(c.cod_unidade = p.codigo)
INNER JOIN endereco AS e ON(c.cod_unidade = e.cod_pessoa)
WHERE c.status IN( 'a', 'f')
AND c.codigo = 651]]>
	</queryString>
