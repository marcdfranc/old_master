	<queryString>
		<![CDATA[SELECT f.codigo AS usuario, d.ctr, d.nome, d.parcelas, f.cpf, f.nascimento,
	n.cep, n.rua_av, n.numero, n.bairro, n.complemento, n.cidade, n.uf,
	d.informacao AS fone, p.cadastro, i.descricao AS fone_unidade,
	e.referencia AS ref_unidade, u.descricao AS unidade
FROM vw_debito_mensalidade AS d
INNER JOIN fisica AS f ON(d.usuario = f.codigo)
INNER JOIN pessoa AS p ON(d.usuario = p.codigo)
INNER JOIN endereco AS n ON(n.cod_pessoa = f.codigo)
INNER JOIN unidade AS u ON(p.cod_unidade = u.codigo)
INNER JOIN pessoa AS e ON(u.codigo = e.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
WHERE i.principal = 's']]>
	</queryString>
