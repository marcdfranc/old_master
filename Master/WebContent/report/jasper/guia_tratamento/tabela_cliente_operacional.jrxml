	<queryString>
		<![CDATA[SELECT t.valor_operacional, t.valor_cliente, t.vigencia, t.aprovacao, s.referencia AS ref_servico,
	s.descricao AS servico, e.descricao AS especialidade,
	CASE
		WHEN e.setor = 'o' THEN 'Odontológica'
		WHEN e.setor = 'l' THEN 'Laboratorial'
		WHEN e.setor = 'm' THEN 'Médica'
		WHEN e.setor = 'h' THEN 'Hospitalar'
	END AS setor,
	u.descricao AS uidade, n.cidade AS cidade_unidade, UPPER(n.uf) AS uf_unidade,
	i.descricao AS fone_unidade, p.referencia AS ref_unidade, f.nome AS administrador
FROM tabela AS t
INNER JOIN unidade AS u ON(t.cod_unidade = u.codigo)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN endereco AS n ON(n.cod_pessoa = u.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
INNER JOIN fisica AS f ON(u.administrador = f.codigo)
INNER JOIN servico AS s ON(t.cod_servico = s.codigo)
INNER JOIN especialidade AS e ON(s.cod_especialidade = e.codigo)
WHERE i.principal = 's'
ORDER BY u.codigo, t.vigencia, s.ref]]>
	</queryString>
