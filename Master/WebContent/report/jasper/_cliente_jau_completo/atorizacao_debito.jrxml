	<queryString>
		<![CDATA[SELECT f.nome, f.cpf, c.ctr, p.cod_unidade, n.referencia AS ref_unidade, j.fantasia,
c.cod_funcionario, j.cnpj
FROM usuario AS u
INNER JOIN contrato AS c ON(u.cod_contrato = c.codigo)
INNER JOIN fisica AS f ON(u.codigo = f.codigo)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN pessoa AS n ON(p.cod_unidade = n.codigo)
INNER JOIN juridica AS j ON(p.cod_unidade = j.codigo)
WHERE u.codigo = $P{COD_USUARIO}]]>
	</queryString>
