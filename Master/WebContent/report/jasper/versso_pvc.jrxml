	<queryString>
		<![CDATA[SELECT u.codigo as cod_unidade, INITCAP(j.razao_social) AS nome, INITCAP(e.rua_av) AS rua_av,
	e.numero, INITCAP(e.bairro) AS bairro, INITCAP(e.cidade ) AS cidade_unidade, UPPER(e.uf) AS uf,
	e.complemento, e.cep, u.site, i.descricao AS fone_unidade
FROM unidade AS u
	INNER JOIN endereco AS e ON(e.cod_pessoa = u.codigo)
	INNER JOIN juridica AS j ON(u.codigo = j.codigo)
	INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
	INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
WHERE i.principal = 's']]>
	</queryString>
