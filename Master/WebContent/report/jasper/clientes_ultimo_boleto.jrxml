	<queryString>
		<![CDATA[select MAX(b.vencimento) AS ultimo_boleto, i.descricao AS fone,
	e.referencia AS ref_unidade, p.cod_unidade,
	CASE
		WHEN f.nome ISNULL THEN j.razao_social
		ELSE f.nome
	END AS nome,
	CASE
		WHEN f.cpf ISNULL THEN j.cnpj
		ELSE f.cpf
	END AS cpf_cnpf,
	CASE
		WHEN u.ctr ISNULL THEN j.codigo
		ELSE u.ctr
	END AS ctr,
	CASE
		WHEN f.cpf ISNULL THEN 'n'
		ELSE 's'
	END AS fisica
from boleto AS b
LEFT JOIN fisica AS f ON(b.cod_pessoa = f.codigo)
LEFT JOIN usuario AS u ON(b.cod_pessoa = u.codigo)
LEFT JOIN juridica AS j ON(b.cod_pessoa = j.codigo)
INNER JOIN informacao AS i ON(b.cod_pessoa = i.cod_pessoa)
INNER JOIN pessoa AS p ON(b.cod_pessoa = p.codigo)
INNER JOIN pessoa AS e ON(p.cod_unidade = e.codigo)
WHERE i.principal = 's'
GROUP BY b.cod_pessoa, f.nome, j.razao_social, f.cpf, j.cnpj, j.codigo, u.ctr,
	i.descricao, e.referencia, p.cod_unidade]]>
	</queryString>
