	<queryString>
		<![CDATA[SELECT c.ctr, c.cod_funcionario, c.codigo AS contrato, p.cod_unidade,
	s.referencia AS ref_unidade, f.nome, f.sexo, f.nascimento, f.estado_civil,
	f.cpf, f.rg, f.naturalidade, f.naturalidade_uf, u.codigo AS cod_usuario,
	u.profissao, u.pai, u.mae, u.qtde_parcela, e.rua_av, e.numero, e.complemento,
	e.bairro, e.cidade, e.uf, e.cep, COUNT(d.*) AS dependente, l.descricao AS plano,
	(SELECT COUNT(*) FROM informacao AS i WHERE i.cod_pessoa = u.codigo) AS rest_info,
	r.cidade AS cidade_unidade, r.uf AS uf_unidade, n.descricao AS unidade,
	i.descricao AS fone_unidade
FROM usuario AS u
INNER JOIN contrato AS c ON(u.cod_contrato = c.codigo)
INNER JOIN fisica AS f ON(u.codigo = f.codigo)
INNER JOIN plano AS l ON(u.cod_plano = l.codigo)
INNER JOIN endereco AS e ON(u.codigo = e.cod_pessoa)
LEFT JOIN dependente AS d ON(u.codigo = d.cod_usuario)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN unidade AS n ON(p.cod_unidade = n.codigo)
INNER JOIN pessoa AS s ON(p.cod_unidade = s.codigo)
INNER JOIN endereco AS r ON(p.cod_unidade = r.cod_pessoa)
INNER JOIN informacao AS i ON(p.cod_unidade = i.cod_pessoa)
WHERE u.codigo = $P{COD_USUARIO}
AND i.principal = 's'
GROUP BY c.ctr, c.cod_funcionario, c.codigo, p.cod_unidade, s.referencia, f.nome, f.sexo,
	f.nascimento, f.estado_civil, f.cpf, f.rg, f.naturalidade, f.naturalidade_uf,
	u.codigo, u.profissao, u.pai, u.mae, u.qtde_parcela, e.rua_av, e.numero,
	e.complemento, e.bairro, e.cidade, e.uf, e.cep, l.descricao, r.cidade, r.uf,
	n.descricao, i.descricao
ORDER BY u.codigo]]>
	</queryString>
