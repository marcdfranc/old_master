	<queryString>
		<![CDATA[SELECT f.nome, c.ctr, l.ultimo_vencimento AS cadastro, i.descricao AS fone, p.cod_unidade, e.referencia AS ref_unidade,
  n.descricao AS unidade
FROM fisica f
INNER JOIN usuario u ON(u.codigo = f.codigo)
INNER join contrato c ON(u.cod_contrato = c.codigo)
INNER JOIN pessoa p ON (f.codigo = p.codigo)
INNER JOIN vw_ultima_mensalidade AS l ON(l.cod_usuario = f.codigo)
LEFT JOIN (SELECT * FROM informacao WHERE principal = 's') i ON(i.cod_pessoa = u.codigo)
INNER JOIN unidade n ON(n.codigo = p.cod_unidade)
INNER JOIN pessoa e ON(e.codigo = p.cod_unidade)
ORDER BY l.ultimo_vencimento DESC, p.cod_unidade
]]>
	</queryString>
