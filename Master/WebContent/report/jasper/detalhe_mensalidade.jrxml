	<queryString>
		<![CDATA[SELECT l.cod_unidade, c.ctr, f.nome, l.data_quitacao, l.valor_pago, l.descricao FROM mensalidade m
INNER JOIN lancamento l ON(m.cod_lancamento = l.codigo)
INNER JOIN usuario u ON(m.cod_usuario = u.codigo)
INNER JOIN fisica f ON(m.cod_usuario = f.codigo)
INNER JOIN contrato c ON(u.cod_contrato = c.codigo)
WHERE l.cod_unidade = 78
AND (l.data_quitacao BETWEEN '2009-05-01' and '2009-06-01')
AND l.descricao IN('mensalidade', 'adesão', 'renovação')
ORDER BY l.descricao, ctr]]>
	</queryString>
