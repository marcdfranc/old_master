	<queryString>
		<![CDATA[SELECT c.codigo, c.inicio, c.fim, c.valor_inicial, c.valor_final, c.usuario,
	u.descricao AS unidade, p.referencia AS ref_unidade,
	i.descricao AS fone_unidade, f.nome, c.cod_unidade
FROM caixa AS c
INNER JOIN unidade AS u ON(c.cod_unidade = u.codigo)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
INNER JOIN pessoa AS e ON(c.usuario = e.username)
INNER JOIN fisica AS f ON(e.codigo = f.codigo)
WHERE i.principal = 's'
AND c.status = 'f'
AND c.codigo = $P{COD_CAIXA}]]>
	</queryString>
