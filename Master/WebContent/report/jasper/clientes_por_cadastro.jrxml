	<queryString>
		<![CDATA[SELECT f.nome, f.cpf, f.nascimento, c.ctr, i.descricao AS fone, p.cadastro,
	u.renovacao, e.referencia AS ref_unidade, p.cod_unidade, j.cnpj,
	n.descricao AS unidade
FROM usuario AS u
INNER JOIN fisica AS f ON(u.codigo = f.codigo)
INNER JOIN contrato AS c ON(u.cod_contrato = c.codigo)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = u.codigo)
INNER JOIN pessoa AS e ON(p.cod_unidade = e.codigo)
INNER JOIN juridica AS j ON(p.cod_unidade = j.codigo)
INNER JOIN unidade AS n ON(p.cod_unidade = n.codigo)
WHERE i.principal = 's'
AND p.cadastro BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}
AND p.cod_unidade = $P{COD_UNIDADE}
ORDER BY p.cod_unidade, p.cadastro DESC, f.nome]]>
	</queryString>
