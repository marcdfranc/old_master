	<style name="Crosstab Data Text" isDefault="false" hAlign="Center"/>
	<queryString>
		<![CDATA[SELECT u.ctr, f.nome, m.cod_lancamento, l.descricao, sum(l.valor) AS valor 
FROM mensalidade AS m
	INNER JOIN fisica AS f ON(m.cod_usuario = f.codigo)
	INNER JOIN usuario AS u ON(u.codigo = f.codigo)
	INNER JOIN lancamento AS l ON(m.cod_lancamento = l.codigo)
GROUP BY l.descricao, m.cod_lancamento, f.nome, u.ctr
UNION
SELECT s.ctr, i.nome, p.cod_lancamento, a.descricao, sum(a.valor) AS valor 
FROM orcamento AS o
	INNER JOIN  parcela_orcamento AS p ON(p.cod_orcamento = o.codigo)
	INNER JOIN fisica AS i ON(o.cod_usuario = i.codigo)
	INNER JOIN usuario AS s ON(i.codigo = s.codigo)
	INNER JOIN lancamento AS a ON(p.cod_lancamento = a.codigo)
GROUP BY a.descricao, p.cod_lancamento, i.nome, s.ctr
ORDER BY ctr]]>
	</queryString>
