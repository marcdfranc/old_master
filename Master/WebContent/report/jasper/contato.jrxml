	<queryString>
		<![CDATA[SELECT i.cod_grupo, g.descricao AS grupo, g.username, c.nome, c.apelido,
	c.aniverssario, e.endereco, e.bairro, e.complemento, e.cidade, e.uf,
	e.cep, p.cod_unidade, n.tipo, n.descricao, n.principal, t.empresa,
	t.cargo, t.setor, t.website, t.fone, d.cep AS cep_empresa,
	d.endereco AS endereco_empresa, d.complemento AS complemento_empresa,
	d.bairro AS bairro_empresa, d.cidade AS cidade_empresa, d.uf AS uf_empresa
FROM itens_grupo as i
INNER JOIN catalogo AS c ON(i.cod_catalogo = c.codigo)
INNER JOIN grupo AS g ON(i.cod_grupo = g.codigo)
INNER JOIN informacao_catalogo as n ON(n.cod_catalogo = c.codigo)
INNER JOIN pessoa AS p ON(p.username = g.username)
LEFT JOIN endereco_catalogo AS e ON(c.cod_endereco = e.codigo)
LEFT JOIN trabalho_contato AS t ON(c.cod_trabalho_contato = t.codigo)
LEFT JOIN endereco_catalogo AS d ON(t.cod_endereco = d.codigo)
WHERE c.codigo = $P{COD_CATALOGO}
ORDER BY g.descricao, c.nome]]>
	</queryString>
