	<queryString>
		<![CDATA[SELECT c.ctr, c.cod_funcionario, p.referencia AS ref_unidade, e.cidade,
	UPPER(e.uf) AS uf, j.cnpj, f.nome, f.cpf
FROM contrato AS c
INNER JOIN usuario AS s ON(c.codigo = s.cod_contrato)
INNER JOIN fisica AS f ON(s.codigo = f.codigo)
INNER JOIN unidade AS u ON(c.cod_unidade = u.codigo)
INNER JOIN pessoa AS p ON(c.cod_unidade = p.codigo)
INNER JOIN juridica AS j ON(c.cod_unidade = j.codigo)
INNER JOIN endereco AS e ON(c.cod_unidade = e.cod_pessoa)
WHERE c.codigo = $P{COD_CONTRATO}]]>
	</queryString>
