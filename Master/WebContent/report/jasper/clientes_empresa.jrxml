	<queryString>
		<![CDATA[select o.ctr, f.nome, f.cpf, p.cadastro, p.ativo, p.cod_unidade,
  CASE 
    WHEN p.ativo='a' THEN 'Ativo'
    WHEN p.ativo='c' THEN 'Cancelado'   
    ELSE 'Bloqueado'
  END AS descricao_status, (
   SELECT descricao FROM informacao AS i 
   WHERE i.principal = 's' 
    AND i.cod_pessoa = u.codigo LIMIT 1) AS informacao, e.ativo AS status,
  CASE 
    WHEN e.ativo='a' THEN 'Ativo'
    WHEN e.ativo='c' THEN 'Cancelado'   
    ELSE 'Bloqueado'
  END AS status_empresa, j.fantasia, j.cnpj 
from contrato_empresa AS c
INNER JOIN usuario AS u ON(c.cod_usuario = u.codigo)
INNER JOIN contrato AS o ON(u.cod_contrato = o.codigo)
INNER JOIN fisica AS f ON(u.codigo = f.codigo)
INNER JOIN pessoa AS p ON(u.codigo = p.codigo)
INNER JOIN juridica AS j ON(c.cod_empresa = j.codigo)
INNER JOIN pessoa AS e ON(j.codigo = e.codigo)
where c.cod_empresa = 2101
order by p.ativo]]>
