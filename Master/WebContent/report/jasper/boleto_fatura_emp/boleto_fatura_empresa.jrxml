	<queryString>
		<![CDATA[SELECT a.cod_fatura, b.codigo, b.vencimento, b.emissao, b.valor AS valor_total,
	l.codigo AS lancamento, l.vencimento, l.valor AS valor_lancamento, l.taxa,
	i.valor_residente, u.codigo AS cod_unidade, o.referencia AS ref_unidade,
	u.descricao AS unidade, j.razao_social, j.cnpj, f.descricao AS fone_unidade,
	r.razao_social AS sacado, r.cnpj AS cnpj_sacado, r.codigo AS ctr, c.cod_banco,
	c.agencia, c.numero AS cc, c.carteira, n.descricao AS banco,
	e.rua_av, e.numero, e.bairro, e.cep, e.complemento, e.cidade, e.uf,
	t.descricao AS tipo_conta, m.codigo AS ref_empresa
FROM boleto AS b
INNER JOIN pessoa AS p ON(b.cod_pessoa = p.codigo)
INNER JOIN endereco AS e ON(e.cod_pessoa = b.cod_pessoa)
INNER JOIN unidade AS u ON(p.cod_unidade = u.codigo)
INNER JOIN juridica AS j ON(j.codigo = u.codigo)
INNER JOIN pessoa AS o ON(o.codigo = u.codigo)
INNER JOIN informacao AS f ON(f.cod_pessoa = u.codigo)
INNER JOIN conta AS c ON(c.cod_pessoa = u.codigo)
INNER JOIN banco AS n ON(c.cod_banco = n.codigo)
INNER JOIN juridica AS r ON(b.cod_pessoa = r.codigo)
INNER JOIN empresa AS m ON(b.cod_pessoa = m.codigo)
INNER JOIN itens_boleto AS i ON(i.cod_boleto = b.codigo)
INNER JOIN lancamento AS l ON(i.cod_lancamento = l.codigo)
INNER JOIN lancamento_fatura_empresa as a ON(a.cod_lancamento = l.codigo)
INNER JOIN tipo_conta AS t ON(l.cod_conta = t.codigo)
WHERE f.principal = 's'
AND b.codigo = $P{COD_BOLETO}
AND c.codigo = 52
ORDER BY u.codigo, l.vencimento]]>
	</queryString>
