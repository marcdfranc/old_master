	<parameter name="FATURA" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT w.cod_unidade, k.referencia AS unidade, o.fantasia AS fantasia_unid,
	f.codigo AS fatura, e.codigo AS cod_empresa, f.data_inicio AS vencimento,
	j.razao_social AS rz_social, j.fantasia, j.cnpj, t.descricao AS ramo,
	n.rua_av AS rua, n.numero, n.bairro AS bairro, n.complemento, n.cep,
	i.descricao AS fone, p.descricao, a.codigo AS lancamento, a.documento,
	a.taxa, a.valor, a.vencimento AS venc_lancamento, z.ctr, z.nome AS cliente,
	z.cpf, r.cidade AS cidade_unidade,
	UPPER(r.uf) AS uf_unidade, m.descricao AS fone_unidade
FROM fatura_empresa as f
INNER JOIN empresa AS e ON(f.cod_empresa = e.codigo)
INNER JOIN pessoa AS w ON(e.codigo = w.codigo)
INNER JOIN juridica AS o ON(o.codigo = w.cod_unidade)
INNER JOIN pessoa AS k ON(w.cod_unidade = k.codigo)
INNER JOIN endereco AS r ON(w.cod_unidade = r.cod_pessoa)
INNER JOIN informacao AS m ON(w.cod_unidade = m.cod_pessoa)
INNER JOIN juridica AS c ON(c.codigo = w.codigo)
INNER JOIN ramo AS t ON(e.cod_ramo = t.codigo)
INNER JOIN lancamento_fatura_empresa as l ON(l.cod_fatura = f.codigo)
INNER JOIN lancamento AS a ON(l.cod_lancamento = a.codigo)
INNER JOIN tipo_conta AS p ON(a.cod_conta = p.codigo)
INNER JOIN (SELECT b.nome, b.cpf, g.ctr, d.cod_lancamento FROM mensalidade AS d
		INNER JOIN fisica AS b ON(d.cod_usuario = b.codigo)
		INNER JOIN usuario AS g ON(g.codigo = b.codigo)
	UNION
	SELECT h.nome, h.cpf, q.ctr, v.cod_lancamento FROM orcamento AS k
		INNER JOIN  parcela_orcamento AS v ON(v.cod_orcamento = k.codigo)
		INNER JOIN fisica AS h ON(k.cod_usuario = h.codigo)
		INNER JOIN usuario AS q ON(h.codigo = q.codigo)
	ORDER BY ctr
	) AS z ON(z.cod_lancamento = a.codigo)
INNER JOIN informacao AS i ON(i.cod_pessoa = e.codigo)
INNER JOIN endereco AS n ON(n.cod_pessoa = e.codigo)
INNER JOIN juridica AS j ON(j.codigo = e.codigo)
WHERE i.principal = 's'
	AND m.principal = 's'
	AND f.codigo = $P{FATURA}
ORDER BY z.ctr, z.nome, a.descricao]]>
	</queryString>
